        <Compile Update="ButtonSpec\ButtonSpecMinimizeRibbon.cs">
            <SubType>Component</SubType>
        </Compile>
        <Compile Update="ButtonSpec\ButtonSpecExpandRibbon.cs">
            <SubType>Component</SubType>
